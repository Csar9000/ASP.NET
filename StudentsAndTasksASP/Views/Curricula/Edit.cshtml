@model Duble2.Models.Curriculum

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">
    <h4>Curriculum</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(model => model.Group_2_GroupNum, "Group_2_GroupNum", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.Group_2_GroupNum, new
                { disabled = "disabled", @readonly = "readonly" })
       
            @Html.ValidationMessageFor(model => model.Group_2_GroupNum, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Subject_SubjectName, "Subject_SubjectName", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("Subject_SubjectName", null, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Subject_SubjectName, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
         
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" id="PlayerID" name="PlayerID" value="@ViewBag.Message" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
