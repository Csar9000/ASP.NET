@model IEnumerable<Duble2.Models.Subject>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeachersFIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <th>
        @Html.DisplayNameFor(model => model.SubjectName)
    </th>
    <td>
        @Html.DisplayFor(modelItem => item.TeachersFIO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Department)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.SubjectName }) |
        @Html.ActionLink("Details", "Details", new { id = item.SubjectName }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.SubjectName })
    </td>
</tr>
}

</table>
